@using Resources
<div class="col-sm-12 col-lg-12">
        <div data-bind="foreach:products.search">
            <div class="col-md-4 col-lg-4 products">
                    <div class="top-box">
                        <h3>Lorem ipsum dolor sit amet</h3>
                        <p>Lorem ipsum</p>
                        <div class="image" data-bind="event: { mouseover: disableDetails}">
                            <img src="@Url.Content("~/Images/")" data-bind="appendToSRCSearch:$data,event: { mouseover: enableDetails}" alt=""/>
                            <div class="mask">
                                <a data-bind="attr:{ href:''},click:electricalDetails">@Resources.Details</a>
                            </div>
                        </div>
                        <div class="price" data-bind="text:'$' +' '+($data.price - $data.reduce)">£480</div>
                        <ul class="list" data-bind="attr:{ href:''},click:addToCart">
                            <li>
                                <img src="@Url.Content("~/Images/Index/plus.png")" alt="" />
                                <ul>
                                    <li>
                                        <a>@Resources.AddtoCart</a>
                                    </li>
                                    <li>
                                        <a class="details" data-bind="attr:{ href:''},click:electricalDetails">@Resources.Details</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
            </div>
        </div>
</div>
<div class="clear"></div>
